</lotusscript></code><code event='initialize'><lotusscript>Sub Initialize
	Dim ns As New NotesSession
	Dim db As NotesDatabase
	Dim vwSuppliersByCall As NotesView
	Dim vwUsers As NotesView
	Dim ndSupplierByCall As NotesDocument
	Dim ndUser As NotesDocument
	
	Set db = ns.Currentdatabase
	Set vwSuppliersByCall = db.Getview("vwSuppliersByCall")
	Set vwUsers = db.Getview("vwUsers")
	
	Set ndSupplierByCall = vwSuppliersByCall.Getfirstdocument()
	While Not ndSupplierByCall Is Nothing 
		If ndSupplierByCall.Hasitem("whoEvaluate") Then 
			Set ndUser = vwUsers.Getdocumentbykey(ndSupplierByCall.whoEvaluate(0), True)
			If Not ndUser Is Nothing Then 
				ndSupplierByCall.whoEvaluate = ndUser.name
				Call ndSupplierByCall.save(True, False)
			End If
		End If
		Set ndSupplierByCall = vwSuppliersByCall.Getnextdocument(ndSupplierByCall)
	Wend
End Sub</lotusscript></code>
